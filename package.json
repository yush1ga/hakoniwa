    "@types/react": "^15.6.19",
    "@types/react-dom": "^0.14.23",
    "@types/underscore": "^1.8.9",
    "@types/webpack": "^3.8.15",
    "apply-loader": "^2.0.0",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.5.2",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "del-cli": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "globule": "^1.2.1",
    "html-loader": "^0.5.4",
    "navigo": "^7.1.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.6",
    "run-script-os": "^1.0.3",
    "sass-loader": "^6.0.7",
    "semantic-ui-react": "^0.68.5",
    "style-loader": "^0.19.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.2",
    "webpack": "^3.12.0"
  }
}
